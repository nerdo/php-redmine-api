{
    "name": "nerdo/redmine-api",
    "type": "library",
    "description": "Redmine API client fork of https://github.com/kbsali/php-redmine-api to support custom authentication header to use with Chiliproject and any other Redmine-compatible APIs that use different header names.",
    "homepage": "https://github.com/nerdo/php-redmine-api",
    "keywords": ["redmine", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Saliou",
            "email": "kevin@saliou.name",
            "homepage": "http://kevin.saliou.name"
        },
        {
            "name": "Dannel Albert"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Redmine": "lib/"
        }
    }
}
